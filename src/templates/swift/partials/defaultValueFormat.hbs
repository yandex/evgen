{{~#eq parameter.defaultValue 'empty_list'~}}
[]
{{~else~}}
    {{~#eq parameter.defaultValue 'empty_dict'~}}
    [:]
    {{~else~}}
        {{~#if (isEnum parameter.type)~}}
            {{~#if (isNumber parameter.defaultValue)~}}
                {{pascalcase (typeFormat parameter)}}.int{{parameter.defaultValue}}
            {{~else~}}
                {{pascalcase (typeFormat parameter)}}.{{camelcase parameter.defaultValue}}
            {{~/if~}}
        {{~else~}}
            {{~#eq parameter.defaultValue null~}}
                nil
            {{~else~}}
                {{~#eq parameter.type 'String'~}}
                    "{{parameter.defaultValue}}"
                {{~else~}}
                    {{parameter.defaultValue}}
                {{~/eq~}}
            {{~/eq~}}
        {{~/if~}}
    {{~/eq~}}
{{~/eq~}}
