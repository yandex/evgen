{{~#eq parameter.defaultValue 'empty_list'~}}
[]
{{~else~}}
    {{~#if (isEnum parameter.type)~}}
        {{~#if (isNumber parameter.defaultValue)~}}
            {{pascalcase (typeFormat parameter defaultTypeName)}}.int{{parameter.defaultValue}}
        {{~else~}}
            {{pascalcase (typeFormat parameter defaultTypeName)}}.{{pascalcase parameter.defaultValue}}
        {{~/if~}}
    {{~else~}}
        {{~#eq parameter.type 'String'~}}
            "{{parameter.defaultValue}}"
        {{~else~}}
            {{~parameter.defaultValue~}}
        {{~/eq~}}
    {{~/if~}}
{{~/eq~}}
