{{~#contains (toArray 'Int' 'Double') (typeFormat parameter)~}}
    "\({{~camelcase parameter.name~}})"
{{~else~}}
    {{~#if (isEnum parameter.type)~}}
        {{~camelcase parameter.name~}}.rawValue
    {{~else~}}
        {{~#if (isConst parameter.type)~}}
            "{{~parameter.type.Const~}}"
        {{~else~}}
            {{~#if (isCustomParameter parameter.type)~}}
                {{~camelcase parameter.name~}}.makeDictionary()
            {{~else~}}
                {{~camelcase parameter.name~}}
            {{~/if~}}
        {{~/if~}}
    {{~/if~}}
{{~/contains~}}
